essa aspettiamo di interfacciarci con essa, al fine di individuare la strada da intraprendere insieme in modo che ne siano tutti soddisfatti, o quanto meno la maggioranza avente diritto di voto.\r\n                                            </p>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                            </section>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </section>\r\n        );\r\n    }\r\n}\r\n\r\nexport default Activity;","import React, { Component } from 'react';\r\n\r\nimport Header from '../components/Header/Header';\r\nimport Roadmap from '../components/Roadmap/Roadmap';\r\nimport Footer from '../components/Footer/Footer';\r\nimport ModalSearch from '../components/Modal/ModalSearch';\r\nimport ModalMenu from '../components/Modal/ModalMenu';\r\nimport Scrollup from '../components/Scrollup/Scrollup';\r\n\r\n\r\nclass Activity extends Component {\r\n    render() {\r\n        return (\r\n            <div className=\"main\">\r\n                <Header />\r\n                <Roadmap />\r\n                <Footer />\r\n                <ModalSearch />\r\n                <ModalMenu />\r\n                <Scrollup />\r\n                {/* lasciato qui perche se no il cronometro va in crash */}\r\n                <div id=\"demo\" class=\"d-none\"></div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default Activity;","import React from 'react';\r\n\r\nimport {Link} from 'react-router-dom';\r\n\r\n\r\nconst Header = () => {\r\n    return (\r\n        <header id=\"header\">\r\n            {/* Navbar */}\r\n            <nav className=\"navbar navbar-expand\">\r\n                <div className=\"container header\">\r\n                    {/* Navbar Brand*/}\r\n                    <Link className=\"navbar-brand\" to=\"/\">\r\n                        <img className=\"navbar-brand-stick