       <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n          <div className=\"star\"></div>\r\n      </div>\r\n\r\n        <s.SpacerSmall />\r\n        <ResponsiveWrapper flex={1} style={{ padding: 24 }} test>\r\n          <s.Container flex={1} jc={\"center\"} ai={\"center\"}>\r\n          </s.Container>\r\n          <s.SpacerLarge />\r\n          <s.Container\r\n            flex={2}\r\n            jc={\"center\"}\r\n            ai={\"center\"}\r\n            style={{\r\n              padding: 24,\r\n              borderRadius: 24,\r\n            }}\r\n          >\r\n            <s.TextTitle\r\n              style={{\r\n                textAlign: \"center\",\r\n                fontSize: 80,\r\n                fontWeight: \"bold\",\r\n                color: \"var(--white-color)\",\r\n              }}\r\n            >\r\n              {data.totalSupply} / {CONFIG.MAX_SUPPLY}\r\n            </s.TextTitle>\r\n            <s.TextDescription\r\n              style={{\r\n                textAlign: \"center\",\r\n                color: \"var(--primary-text)\",\r\n              }}\r\n            >\r\n              <StyledLink target={\"_blank\"} href={CONFIG.SCAN_L